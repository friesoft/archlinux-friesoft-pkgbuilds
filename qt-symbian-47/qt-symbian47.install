post_install()
{
  echo 'Set those environment variables to use Qt for Symbian'
  echo 'export PATH=/opt/gnupoc1_17/bin:$PATH'
  echo 'export EPOCROOT=/opt/gnupoc1_17/symbian-sdks/5.0/'
  echo 'export PATH=/opt/qt470symbian/bin:$PATH'
  echo '# Before using Qt for symbian QMAKESPEC must be unset (which is not set by default)'
  echo 'unset QMAKESPEC'
}

